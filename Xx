#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double x, result;

    cout << "Enter a value for x: ";
    cin >> x;

    if (x <= 2) {
        result = 4 * pow(x, 3) - pow(sin(x), 2);
        cout << "The result is: " << fixed << result << endl;
    } else {
        cout << "Error: x must be less than or equal to 2." << endl;
    }

    return 0;
}
